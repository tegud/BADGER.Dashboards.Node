{
    "name": "Moonstick (Main)",
    "components": [
        {
            "type": "HealthCheckSentinel",
            "alertName": "healthCheck.serviceHealth-live",
            "group": "Moonstick",
            "title": "Status",
            "host": "sentinel.laterooms.com",
            "port": 1237,
            "span": 3
        },
        {
            "type": "HealthCheckSentinel",
            "alertName": "healthCheck.serviceHealth-live",
            "group": "APIs",
            "title": "API Status",
            "host": "sentinel.laterooms.com",
            "port": 1237,
            "span": 3
        },
        {
            "type": "LoadBalancerStatus",
            "poolId": "moonstick_http",
            "title": "Moonstick Servers",
            "host": "10.44.35.62",
            "port": 1234,
            "span": 3
        },
        {
            "type": "LoadBalancerStatus",
            "poolId": "Varnish",
            "title": "Varnish Servers",
            "host": "10.44.35.62",
            "port": 1234,
            "span": 3
        },
        {
            "type": "SharedDataStore",
            "storeId": "LRRequestsData",
            "dataSource": "elasticsearch",
            "host": "http://logs.elasticsearch.laterooms.io:9200",
            "timeProperties": [
                "query.filtered.filter.bool.must.0.range.@timestamp",
                "aggs.requests.date_histogram.extended_bounds"
            ],
            "intervalProperties": [
                "aggs.requests.date_histogram.interval"
            ],
            "queries": {
                "modifiers": {
                    "value": {}
                },
                "query": {
                    "query": {
                        "filtered": {
                            "filter": {
                                "bool": {
                                    "must": [
                                        {
                                            "range": {
                                                "@timestamp": {
                                                    "from": "now-60m"
                                                }
                                            }
                                        },
                                        {
                                            "term": {
                                                "type": "lr_varnish_request"
                                            }
                                        },
                                        {
                                            "term": {
                                                "is_moonstick": true
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "aggs": {
                        "requests": {
                            "date_histogram": {
                                "min_doc_count": 0,
                                "extended_bounds": {
                                    "min": "now-60m",
                                    "max": "now"
                                },
                                "field": "@timestamp",
                                "interval": "1m"
                            },
                            "aggs": {
                                "home": {
                                    "filter": {
                                        "term": {
                                            "url_page_type.raw": "home"
                                        }
                                    },
                                    "aggs": {
                                        "beacon": {
                                            "filter": {
                                                "term": {
                                                    "tags": "beacon"
                                                }
                                            },
                                            "aggs": {
                                                "page_complete": {
                                                    "stats": {
                                                        "field": "timing_done"
                                                    }
                                                },
                                                "percentiles": {
                                                    "percentiles": {
                                                        "field": "timing_done"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                },
                                "hotel-details": {
                                    "filter": {
                                        "and" : [
                                            {
                                                "term": {
                                                    "url_page_type.raw": "hotel-details"
                                                }
                                            },
                                            {
                                                "not": {
                                                    "term": {
                                                        "url_page": "reviews"
                                                    }
                                                }
                                            }
                                        ]
                                    },
                                    "aggs": {
                                        "beacon": {
                                            "filter": {
                                                "term": {
                                                    "tags": "beacon"
                                                }
                                            },
                                            "aggs": {
                                                "page_complete": {
                                                    "stats": {
                                                        "field": "timing_done"
                                                    }
                                                },
                                                "percentiles": {
                                                    "percentiles": {
                                                        "field": "timing_done"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                },
                                "search": {
                                    "filter": {
                                        "term": {
                                            "url_page_type.raw": "search"
                                        }
                                    },
                                    "aggs": {
                                        "beacon": {
                                            "filter": {
                                                "term": {
                                                    "tags": "beacon"
                                                }
                                            },
                                            "aggs": {
                                                "page_complete": {
                                                    "stats": {
                                                        "field": "timing_done"
                                                    }
                                                },
                                                "percentiles": {
                                                    "percentiles": {
                                                        "field": "timing_done"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "size": 0
                }
            },
            "mappings": [
                {
                    "type": "extractFromDateHistogram",
                    "aggregateName": "requests",
                    "fields": {
                        "homeRequests": "home.doc_count",
                        "hotelDetailsRequests": "hotel-details.doc_count",
                        "searchRequests": "search.doc_count",
                        "homePageComplete": "home.beacon.percentiles.values.50|0",
                        "hotelDetailsPageComplete": "hotel-details.beacon.percentiles.values.50|0",
                        "searchPageComplete": "search.beacon.percentiles.values.50|0"
                    }
                }
            ]
        },
        {
            "type": "LineGraphAndCounter",
            "title": "LR Requests",
            "span": 6,
            "storeId": "LRRequestsData",
            "summaryText": "Requests per minute",
            "graph": {
                "window": {
                    "skip": 0,
                    "take": 60
                },
                "lines": [
                    {
                        "id": "home-requests",
                        "color": "#411085",
                        "value": "value.homeRequests"
                    },
                    {
                        "id": "hotel-details-requests",
                        "color": "green",
                        "value": "value.hotelDetailsRequests"
                    },
                    {
                        "id": "search-requests",
                        "color": "orange",
                        "value": "value.searchRequests"
                    }
                ]
            },
            "counter": {
                "title": "Requests per min (Last whole)",
                "className": "commission-graph-counter",
                "window": {
                    "skip": 1,
                    "take": 10
                },
                "precision": 0,
                "counters": [
                    {
                        "id": "home-requests",
                        "color": "#411085",
                        "value": "value.homeRequests",
                        "text": "Home"
                    },
                    {
                        "id": "hotel-details-requests",
                        "color": "green",
                        "value": "value.hotelDetailsRequests",
                        "text": "Hotel Details"
                    },
                    {
                        "id": "search-requests",
                        "color": "orange",
                        "value": "value.searchRequests",
                        "text": "Search"
                    }
                ]
            }
        },
        {
            "type": "LineGraphAndCounter",
            "title": "Server Errors",
            "span": 6,
            "summaryText": "Server Errors per one minute on Moonstick",
            "dataSource": "elasticsearch",
            "host": "http://logs.elasticsearch.laterooms.io:9200",
            "aggregateProperty": "errorsbytime",
            "timeProperties": [
                "query.filtered.filter.bool.must.0.range.@timestamp",
                "aggs.errorsbytime.date_histogram.extended_bounds"
            ],
            "intervalProperties": [
                "aggs.errorsbytime.date_histogram.interval"
            ],
            "query": {
                "query": {
                    "filtered": {
                        "filter": {
                            "bool": {
                                "must": [
                                    {
                                        "range": {
                                            "@timestamp": {
                                                "from": "now-60m"
                                            }
                                        }
                                    },
                                    {
                                        "term": { "type": "ms_errors" }
                                    }
                                ]
                            }
                        }
                    }
                },
                "aggs": {
                    "errorsbytime": {
                        "date_histogram": {
                            "min_doc_count": 0,
                            "extended_bounds": {
                                "min": "now-60m",
                                "max": "now"
                            },
                            "field": "@timestamp",
                            "interval": "1m"
                        }
                    }
                },
                "size": 0
            },
            "mappings": [
                {
                    "type": "extractFromDateHistogram",
                    "aggregateName": "errorsbytime",
                    "fields": {
                        "errors": "doc_count"
                    }
                }
            ],
            "graph": {
                "lines": [
                    {
                        "color": "#f00",
                        "id": "errors",
                        "value": "query.errors"
                    }
                ]
            },
            "counter": {
                "title": "Errors in last 10mins",
                "className": "commission-graph-counter",
                "precision": 0,
                "value": "query.errors"
            }
        },
        {
            "type": "LineGraphAndCounter",
            "title": "LR Page Load Time (50th Percentile)",
            "span": 6,
            "summaryText": "",
            "storeId": "LRRequestsData",
            "graph": {
                "lines": [
                    {
                        "id": "home",
                        "color": "#411085",
                        "value": "value.homePageComplete"
                    },
                    {
                        "id": "hotel-details",
                        "color": "green",
                        "value": "value.hotelDetailsPageComplete"
                    },
                    {
                        "id": "search",
                        "color": "orange",
                        "value": "value.searchPageComplete"
                    }
                ]
            },
            "counter": {
                "title": "Average Time to Page Load",
                "className": "counter-smallest-text",
                "type": "average",
                "window": {
                    "skip": 1,
                    "take": 10
                },
                "suffix": "ms",
                "precision": 0,
                "counters": [
                    {
                        "id": "home",
                        "color": "#411085",
                        "value": "value.homePageComplete",
                        "text": "Home"
                    },
                    {
                        "id": "hotel-details",
                        "color": "green",
                        "value": "value.hotelDetailsPageComplete",
                        "text": "Hotel Details"
                    },
                    {
                        "id": "search",
                        "color": "orange",
                        "value": "value.searchPageComplete",
                        "text": "Search"
                    }
                ]
            }
        },
        {
            "type": "LineGraphAndCounter",
            "title": "Client Side Errors",
            "span": 6,
            "summaryText": "Client Errors per one minute on Moonstick",
            "dataSource": "elasticsearch",
            "host": "http://logs.elasticsearch.laterooms.io:9200",
            "aggregateProperty": "errorsbytime",
            "timeProperties": [
                "query.filtered.filter.bool.must.0.range.@timestamp",
                "aggs.errorsbytime.date_histogram.extended_bounds"
            ],
            "intervalProperties": [
                "aggs.errorsbytime.date_histogram.interval"
            ],
            "query": {
                "query": {
                    "filtered": {
                        "filter": {
                            "bool": {
                                "must": [
                                    {
                                        "range": {
                                            "@timestamp": {
                                                "from": "now-60m"
                                            }
                                        }
                                    },
                                    {
                                        "terms": {
                                            "type": [
                                                "lr_varnish_request"
                                            ]
                                        }
                                    },
                                    {
                                        "term": {
                                            "type": "lr_varnish_request"
                                        }
                                    },
                                    {
                                        "term": {
                                            "url_path.raw": "/beacon/moonstick-clienterrors"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "aggs": {
                    "errorsbytime": {
                        "date_histogram": {
                            "min_doc_count": 0,
                            "extended_bounds": {
                                "min": "now-60m",
                                "max": "now"
                            },
                            "field": "@timestamp",
                            "interval": "1m"
                        }
                    }
                },
                "size": 0
            },
            "mappings": [
                {
                    "type": "extractFromDateHistogram",
                    "aggregateName": "errorsbytime",
                    "fields": {
                        "lrerrors": "doc_count"
                    }
                }
            ],
            "graph": {
                "lines": [
                    {
                        "color": "#411085",
                        "id": "lr-errors",
                        "value": "query.lrerrors"
                    }
                ]
            },
            "counter": {
                "title": "Client Errors in last 10mins",
                "className": "commission-graph-counter",
                "precision": 0,
                "value": "query.lrerrors"
            }
        }
    ]
}
