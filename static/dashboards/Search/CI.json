{
    "name": "Search CI",
    "components": [
    {
          "type": "SharedDataStore",
          "title": "Search CI Data",
          "storeId": "SearchCIData",
          "dataSource": "elasticsearch",
          "host": "http://logs.elasticsearch.laterooms.io:9200",
          "index": "releases-${YYYY}.${MM}",
          "timeProperties": [
            "query.filtered.filter.bool.must.0.range.@timestamp"
          ],
          "defaultTimeFrame": {
            "timeFrame": 0,
            "units": "daysAgo"
          },
          "queries": {
            "modifiers": {
              "today": { }
            },
            "query": {
              "query": {
                "filtered": {
                  "filter": {
                    "bool": {
                      "must": [
                        {
                          "range" : {
                            "@timestamp" : { }
                          }
                        },
                        {
                          "term" : {
                            "group.raw" : "Product-Team-Search"
                          }
                        }
                      ]
                    }
                  }
                }
              },
              "size": 1000
            }
          },
          "mappings": []
        },
        {
            "title": "Pending PRs",
            "type": "PullRequests",
            "span": "4",
            "sourceUrl": "http://162.243.65.207/pullRequests?matching=LateRoomsGroup/search-|LateRoomsGroup/pa-automartin|LateRoomsGroup/orchestration-layer|Corp-CM|partners-api|keywords-api|seo-content-api|adverts-api|pa-"
        },
        {
            "title": "In Progress",
            "type": "InProgressReleases",
            "span": "5",
            "storeId": "SearchCIData",
            "defaultSortOrder": "Descending"
        },
        {
            "title": "Completed",
            "type": "CompletedReleases",
            "span": "3",
            "storeId": "SearchCIData"
        }
    ]
}